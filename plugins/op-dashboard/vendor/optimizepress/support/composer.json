{
    "name": "optimizepress/support",
    "description": "Support stuff for OptimizePress",
    "type": "library",
    "authors": [
        {
            "name": "Boris Strahija",
            "email": "bstrahija@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "OptimizePress\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        { "type": "composer", "url": "https://satis.influendo.com/" }
    ],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    }
}
